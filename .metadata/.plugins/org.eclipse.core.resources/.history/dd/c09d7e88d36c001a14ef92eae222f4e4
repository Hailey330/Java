package array03;

//char arr [] = {'가', '나', '다', '라', '마'};
//새로운 배열에 복사해서 만들 것. 
// for 문을 이용하여 {마 라 다 나 가} 역순으로 만들기. 
// for 문으로 출력
public class Test {

	public static void main(String[] args) {
		char arr[] = {'가', '나', '다', '라', '마'};
		
		
		for (int i = 4; i < arr.length; i--) {
			System.out.println(arr[i]);
		}
	}

}
