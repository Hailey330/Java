package array03;

// 숫자 1000000000 를 입력받고 3자리마다 콤마를 찍어서 출력! 1,000,000,000

public class countTest {
	public static void main(String[] args) {
		int num = 1000000000;
		System.out.println("1"+","+"000"+ "," + "000" + "," + "000");
		
		for (int i = 0; i < args.length; i++) {
			System.out.println(num);
		}
	}

}
