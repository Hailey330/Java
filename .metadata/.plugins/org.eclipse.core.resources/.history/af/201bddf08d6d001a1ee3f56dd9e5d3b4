		while (confirm) {
			System.out.println("수를 결정하였습니다. 맞추어 보세요");
			System.out.println();
			int num = sc.nextInt();
			
			for (int i = 0; i < 100; i++) {
				System.out.println(i + ">>" + num);
				if (confirm) {

				}
			}
		}
	}

}
