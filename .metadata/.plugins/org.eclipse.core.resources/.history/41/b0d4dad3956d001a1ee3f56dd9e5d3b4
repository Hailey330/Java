package array03;

//*
//**
//***
//****

public class StarEx01 {

	void printStars(int i) {
		for (int j = 0; j < i; j++) {
			System.out.print("*");
		}
		System.out.println();
	}

	public static void main(String[] args) {
		System.out.println("*"); // 1개
		System.out.println("**"); // 2개
		System.out.println("***"); // 3개
		System.out.println("****"); // 4개
	}

}
