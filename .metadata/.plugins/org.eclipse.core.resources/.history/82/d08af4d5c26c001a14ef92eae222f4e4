package array03;

import java.util.Scanner;

public class Gugudan2 {

	// 사용자로부터 입력받은 구구단 프로그램 완성!

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();

		for (int i = 1; i < 10; i++) {
			System.out.println(num + "*" + i + "=" + (num * i));
		}
	
		for (int i = 0; i <= 10; i++) {
			for (int j = 0; j <= i; j++) {
				System.out.println(i* "*");
				
			}System.out.println();
			
		}
	}
	
}
